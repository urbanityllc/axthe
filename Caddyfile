# re: matcher http://caddyserver.com/docs/caddyfile-tutorial
# example matcher of static and dynamic combined
:9090 {
	root * ./caddy/dev
	file_server

	# the magic of matcher is here:
	reverse_proxy /api/* :8080
	reverse_proxy /dyn/* :8080
}



